{
	"name": "portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev --host",
		"format": "prettier --plugin='prettier-plugin-svelte' --ignore-path .gitignore --write .",
		"lint:format": "prettier --plugin='prettier-plugin-svelte' --ignore-path .gitignore --check .",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint": "npm run lint:format && npm run lint:eslint",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@svelte-plugins/tooltips": "^0.1.8",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.27.1",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.0",
		"fast-xml-parser": "^4.3.2",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"super-sitemap": "^0.14.2",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"sveltekit-i18n": "^2.4.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.1"
	},
	"type": "module"
}
